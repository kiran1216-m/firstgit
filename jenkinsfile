pipeline{
   agent any
     stages{
        stage('fetching stage'){
          steps{
            git branch: 'main', url: 'https://github.com/kiran1216-m/maven_cont.git'
            echo 'fetching the code from github'
                }
             }
        stage('second stage'){
          steps{
            echo 'this is a build stage'
               }
             }
        stage('third stage'){
             steps{
                echo 'this is a testing stage'
                   }
                 }
        stage('fourth stage'){
            steps{
               echo 'this is a deploy stage'
                 }
              } 
         }
    }
